{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "xslt-js",
			"label": "My_build_task",
			"xsltFile": "${workspaceFolder}/Lecture_1.xslt",
			"xmlSource": "${workspaceFolder}/Lecture_1.xml",
			"resultPath": "${workspaceFolder}/xsl-out/result1.xml",
			"group": "build",
			"problemMatcher": [
				"$saxon-xslt-js"
			]
		},
		{
			"type": "xslt-js",
			"label": "default_build",
			"xsltFile": "${workspaceFolder}/Lecture_9.xslt",
			"xmlSource": "${workspaceFolder}/Lecture_9.xml",
			"resultPath": "${workspaceFolder}/xsl-out/result9.html",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$saxon-xslt-js"
			]
		},
		{
			"type": "xslt-js",
			"label": "xslt-js: Saxon-JS Transform (New)",
			"xsltFile": "${command:xslt-xpath.pickXsltFile}",
			"xmlSource": "${command:xslt-xpath.pickXmlSourceFile}",
			"resultPath": "${workspaceFolder}/xsl-out/result1.xml",
			"group": {
				"kind": "build"
			},
			"problemMatcher": [
				"$saxon-xslt-js"
			]
		},
		{
			"type": "xslt-js",
			"label": "xslt-js: xslt-js: Saxon-JS Transform (New)",
			"xsltFile": "${command:xslt-xpath.pickXsltFile}",
			"xmlSource": "${command:xslt-xpath.pickXmlSourceFile}",
			"resultPath": "${workspaceFolder}/xsl-out/result1.xml",
			"group": {
				"kind": "build"
			},
			"problemMatcher": [
				"$saxon-xslt-js"
			]
		}
	]
}